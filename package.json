{
  "name": "peoples-2020",
  "version": "1.0.0",
  "author": "Laura Brown",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "dev": "yarn clean && webpack --colors --mode development && theme deploy && concurrently --prefix name --names 'webpack,theme watch' --prefix-colors yellow,blue \"webpack --watch --colors --mode development\" \"theme watch\"",
    "start": "yarn dev",
    "download:settings": "theme download config/settings_data.json --dir=src/ --no-ignore",
    "build": "yarn clean && webpack --mode production",
    "deploy": "yarn build && theme deploy"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^3.0.3",
    "@shopify/theme-cart": "^3.0.3",
    "@shopify/theme-currency": "^3.0.3",
    "@shopify/theme-images": "^4.0.0",
    "@shopify/theme-product": "^3.0.3",
    "@shopify/theme-product-form": "^3.0.3",
    "@shopify/theme-sections": "^3.0.3",
    "core-js": "^3.6.4",
    "lazysizes": "^5.2.0",
    "lodash": "^4.17.19",
    "quicklink": "^2.0.0-alpha",
    "regenerator-runtime": "^0.13.5",
    "slugify": "^1.4.0",
    "svbstrate": "^4.1.1",
    "swiper": "^6.1.1",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-unique-id": "^3.1.0",
    "vue2-touch-events": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cssnano-webpack-plugin": "^1.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "normalize.css": "^8.0.1",
    "postcss-discard-comments": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-lint-webpack": "^1.0.2",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
