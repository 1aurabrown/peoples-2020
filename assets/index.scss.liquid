@font-face {
  font-family: "Favorit Mono";
  src: url({{ "FavoritMonoStd-Regular.eot" | asset_url }});
  src: url({{ "FavoritMonoStd-Regular.eot?#iefix" | asset_url }}) format("embedded-opentype"),
       url({{ "FavoritMonoStd-Regular.woff2" | asset_url }}) format("woff2"),
       url({{ "FavoritMonoStd-Regular.woff" | asset_url }}) format("woff");
  font-weight: normal;
  font-style: normal;
}
